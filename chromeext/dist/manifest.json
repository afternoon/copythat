{
  "manifest_version": 2,

  "name": "CopyThat",
  "description": "Request website copy changes from your tech team",
  "version": "0.1.0",

  "key": "",

  "permissions": [
    "activeTab",
    "identity",
    "https://accounts.google.com/*",
    "https://www.googleapis.com/*"
  ],

  "oauth2": {
    "client_id": "528818435148-q5r9v0u9o77dmdngumes3cfkljj6v88m.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/plus.login"
    ]
  },

  "icons": {
    "48": "icons/off48.png",
    "128": "icons/off128.png"
  },

  "background": {
    "scripts": ["copythat.js"],
    "persistent": false
  },

  "page_action": {
    "default_title": "Suggest copy changes",
    "default_icon": "icons/off38.png"
  }
}
